   "tokenColors": [
      {
         "settings": {
            "foreground": "#eeffffff",
            "background": "#263238ff"
         }
      },
      {
         "name": "Comment",
         "scope": [
            "punctuation.definition.comment",
            "comment",
            "punctuation.definition.tag"
         ],
         "settings": {
            "foreground": "#647e82"
         }
      },
      {
         "scope": [
            "punctuation",
            "meta.brace",
            "constant.character",
            "constant.escape",
            "text.html.markdown",
            "markup.quote.markdown meta.paragraph.markdown",
            "punctuation.definition.list_item.markdown",
            "keyword.begin.tag.ejs",
            "support.function.dom"
         ],
         "settings": {
            "foreground": "#bbc3c4"
         }
      },
      {
         "scope": [
            "keyword",
            "storage.type.class",
            "meta.tag.sgml",
            "markup.deleted.git_gutter",
            "storage.modifier",
            "text.html.markdown markup.inline.raw.markdown",
            "string.other.link.title.markdown",
            "string.other.link.description.title.markdown",
            "beginning.punctuation.definition.list.markdown",
            "markup.underline.link.markdown",
            "markup.underline.link.image.markdown",
            "constant.other.reference.link.markdown"
         ],
         "settings": {
            "foreground": "#df769b"
         }
      },
      {
         "scope": [
            "punctuation.accessor",
            "source.python punctuation.section.function",
            "source.python punctuation.separator.period",
            "source.python punctuation.separator.period",
            "keyword.control",
            "constant.other.color",
            "keyword.other.template",
            "keyword.other.substitution",
            "storage.type.function.arrow"
         ],
         "settings": {
            "foreground": "#df769b",
            "fontStyle": "bold"
         }
      },
      {
         "scope": [
            "entity.other.attribute-name.pseudo-class",
            "entity.other.attribute-name.pseudo-element",
            "tag.decorator.js entity.name.tag.js",
            "tag.decorator.js punctuation.definition.tag.js",
            "beginning.punctuation.definition.quote.markdown",
            "markup.quote",
            "markup.italic"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#df769b"
         }
      },
      {
         "name": "Markup - Underline",
         "scope": [
            "markup.underline"
         ],
         "settings": {
            "fontStyle": "underline",
            "foreground": "#df769b"
         }
      },
      {
         "name": "Markup - Strike",
         "scope": [
            "markup.strike"
         ],
         "settings": {
            "fontStyle": "strike",
            "foreground": "#df769b"
         }
      },
      {
         "name": "Variables, Entity Name",
         "scope": [
            "variable",
            "variable.object",
            "variable.other",
            "variable.parameter",
            "string constant.other.placeholder",
            "source.css.scss variable",
            "variable.other.object",
            "source.cs entity.name.variable",
            "entity.name.module",
            "variable.import.parameter",
            "variable.other.class",
            "meta.function-call.arguments",
            "meta.toc-list.id.html",
            "source.json meta.structure.dictionary.json support.type.property-name.json",
            "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown",
            "markdown.heading",
            "markup.heading | markup.heading entity.name",
            "markup.heading.markdown punctuation.definition.heading.markdown"
         ],
         "settings": {
            "foreground": "#e4b781"
         }
      },
      {
         "name": "ES7 Bind Operator",
         "scope": [
            "source.js constant.other.object.key.js string.unquoted.label.js"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#e4b781"
         }
      },
      {
         "name": "MARKDOWN - Heading Name Section",
         "scope": [
            "entity.name.section.markdown",
            "markup.heading.setext.1.markdown",
            "markup.heading.setext.2.markdown"
         ],
         "settings": {
            "foreground": "#e4b781",
            "fontStyle": "bold"
         }
      },
      {
         "name": "Markdown - Blockquote",
         "scope": [
            "markup.quote punctuation.definition.blockquote.markdown"
         ],
         "settings": {
            "background": "#e4b781",
            "foreground": "#e4b781"
         }
      },
      {
         "name": "Markdown - Separator",
         "scope": [
            "meta.separator"
         ],
         "settings": {
            "fontStyle": "bold",
            "background": "#00000050",
            "foreground": "#e4b781"
         }
      },
      {
         "scope": [
            "constant",
            "variable.other.constant",
            "source.js support.constant",
            "entity.other.attribute-name.class",
            "punctuation.definition.entity",
            "support.variable.property.dom",
            "punctuation.definition.template-expression",
            "constant.character.entity",
            "support.function.dom"
         ],
         "settings": {
            "foreground": "#cc9900"
         }
      },
      {
         "scope": [
            "text.html.basic entity.other.attribute-name.html",
            "text.html.basic entity.other.attribute-name",
            "entity.other.attribute-name"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#cc9900"
         }
      },
      {
         "name": "Markup - Bold",
         "scope": [
            "markup.bold",
            "markup.bold string"
         ],
         "settings": {
            "fontStyle": "bold",
            "foreground": "#cc9900"
         }
      },
      {
         "name": "Markup - Bold-Italic",
         "scope": [
            "markup.bold markup.italic",
            "markup.italic markup.bold",
            "markup.quote markup.bold",
            "markup.bold markup.italic string",
            "markup.italic markup.bold string",
            "markup.quote markup.bold string"
         ],
         "settings": {
            "fontStyle": "bold italic",
            "foreground": "#cc9900"
         }
      },
      {
         "scope": [
            "variable.function",
            "keyword.other.special-method",
            "support.function",
            "support.function.console",
            "support.type.vendored",
            "source.css support.type.property-name",
            "source.sass support.type.property-name",
            "source.scss support.type.property-name",
            "source.less support.type.property-name",
            "source.stylus support.type.property-name",
            "source.postcss support.type.property-name",
            "string.other.link.title.markdown",
            "string.other.link.description.markdown",
            "string.other.link.description.title.markdown",
            "keyword.other.unit"
         ],
         "settings": {
            "foreground": "#16a3b6"
         }
      },
      {
         "scope": [
            "entity.name.function",
            "entity.name.type",
            "source.cs entity.name.type",
            "support",
            "support.other.variable",
            "string.other.link"
         ],
         "settings": {
            "foreground": "#16a3b6",
            "fontStyle": "none"
         }
      },
      {
         "name": "Invalid, Exception",
         "scope": [
            "invalid",
            "invalid.illegal",
            "support.type.exception"
         ],
         "settings": {
            "foreground": "#e34e1c"
         }
      },
      {
         "scope": [
            "variable.language",
            "source.cs keyword.other.this",
            "markup.italic.markdown",
            "variable.parameter.function.language.special.self.python"
         ],
         "settings": {
            "foreground": "#e97749"
         }
      },
      {
         "scope": [
            "storage.type",
            "storage.type.function",
            "entity.name.tag"
         ],
         "settings": {
            "fontStyle": "none",
            "foreground": "#e97749"
         }
      },
      {
         "name": "Numbers, Booleans, CSS - Units",
         "scope": [
            "constant.numeric",
            "constant.language"
         ],
         "settings": {
            "foreground": "#7060eb"
         }
      },
      {
         "name": "String, Symbols",
         "scope": [
            "string",
            "constant.other.symbol",
            "constant.other.key",
            "markup.inserted.git_gutter",
            "punctuation.definition.string",
            "source.css support.constant",
            "markup.raw.block",
            "markup.inline.raw.string.markdown"
         ],
         "settings": {
            "fontStyle": "none",
            "foreground": "#58CC6D"
         }
      },
      {
         "name": "Class, Support, Annotations",
         "scope": [
            "entity.name",
            "entity.name.type",
            "support.orther.namespace.use.php",
            "meta.use.php",
            "support.other.namespace.php",
            "markup.changed.git_gutter",
            "source.css support.function",
            "support.type",
            "support.class",
            "support.variable",
            "support.function",
            "meta.array support.variable.property",
            "meta.objectliteral support.variable.property",
            "source.js punctuation.section.embedded",
            "support.function.construct"
         ],
         "settings": {
            "foreground": "#49d6e9"
         }
      },
      {
         "name": "URL",
         "scope": [
            "*url*",
            "*link*",
            "*uri*"
         ],
         "settings": {
            "fontStyle": "underline"
         }
      },
      {
         "scope": "token.info-token",
         "settings": {
            "foreground": "#748FFC"
         }
      },
      {
         "scope": "token.warn-token",
         "settings": {
            "foreground": "#ff901a"
         }
      },
      {
         "scope": "token.error-token",
         "settings": {
            "foreground": "#e34e1c"
         }
      },
      {
         "scope": "token.debug-token",
         "settings": {
            "foreground": "#df769b"
         }
      },
      {
         "name": "Italic",
         "scope": [
            "modifier",
            "this",
            "comment",
            "storage.modifier.js",
            "storage.type.function",
            "punctuation.definition.comment",
            "entity.other.attribute-name",
            "variable.language",
            "support.type.vendored",
            "support.constant.vendored",
            "source.cs keyword.other.this"
         ],
         "settings": {
            "fontStyle": "italic"
         }
      },
      {
         "name": "Bold",
         "scope": [
            "keyword"
         ],
         "settings": {
            "fontStyle": "bold"
         }
      },
      {
         "name": "Normal",
         "scope": [
            "keyword.begin.tag.ejs"
         ],
         "settings": {
            "fontStyle": "none"
         }
      }
   ]
}