   "tokenColors": [
      {
         "settings": {
            "foreground": "#eeffffff",
            "background": "#263238ff"
         }
      },
      {
         "name": "Comment",
         "scope": [
            "punctuation.definition.comment",
            "comment",
            "punctuation.definition.tag"
         ],
         "settings": {
            "foreground": "#8ca4a6"
         }
      },
      {
         "scope": [
            "punctuation",
            "meta.brace",
            "constant.character",
            "constant.escape",
            "text.html.markdown",
            "markup.quote.markdown meta.paragraph.markdown",
            "punctuation.definition.list_item.markdown",
            "keyword.begin.tag.ejs",
            "support.function.dom"
         ],
         "settings": {
            "foreground": "#1a4b51"
         }
      },
      {
         "scope": [
            "keyword",
            "storage.type.class",
            "meta.tag.sgml",
            "markup.deleted.git_gutter",
            "storage.modifier",
            "text.html.markdown markup.inline.raw.markdown",
            "string.other.link.title.markdown",
            "string.other.link.description.title.markdown",
            "beginning.punctuation.definition.list.markdown",
            "markup.underline.link.markdown",
            "markup.underline.link.image.markdown",
            "constant.other.reference.link.markdown"
         ],
         "settings": {
            "foreground": "#ec6594"
         }
      },
      {
         "scope": [
            "punctuation.accessor",
            "source.python punctuation.section.function",
            "source.python punctuation.separator.period",
            "source.python punctuation.separator.period",
            "keyword.control",
            "constant.other.color",
            "keyword.other.template",
            "keyword.other.substitution",
            "storage.type.function.arrow"
         ],
         "settings": {
            "foreground": "#ec6594",
            "fontStyle": "bold"
         }
      },
      {
         "scope": [
            "entity.other.attribute-name.pseudo-class",
            "entity.other.attribute-name.pseudo-element",
            "tag.decorator.js entity.name.tag.js",
            "tag.decorator.js punctuation.definition.tag.js",
            "beginning.punctuation.definition.quote.markdown",
            "markup.quote",
            "markup.italic"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#ec6594"
         }
      },
      {
         "name": "Markup - Underline",
         "scope": [
            "markup.underline"
         ],
         "settings": {
            "fontStyle": "underline",
            "foreground": "#ec6594"
         }
      },
      {
         "name": "Markup - Strike",
         "scope": [
            "markup.strike"
         ],
         "settings": {
            "fontStyle": "strike",
            "foreground": "#ec6594"
         }
      },
      {
         "name": "Variables, Entity Name",
         "scope": [
            "variable",
            "variable.object",
            "variable.other",
            "variable.parameter",
            "string constant.other.placeholder",
            "source.css.scss variable",
            "variable.other.object",
            "source.cs entity.name.variable",
            "entity.name.module",
            "variable.import.parameter",
            "variable.other.class",
            "meta.function-call.arguments",
            "meta.toc-list.id.html",
            "source.json meta.structure.dictionary.json support.type.property-name.json",
            "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown",
            "markdown.heading",
            "markup.heading | markup.heading entity.name",
            "markup.heading.markdown punctuation.definition.heading.markdown"
         ],
         "settings": {
            "foreground": "#e9a149"
         }
      },
      {
         "name": "ES7 Bind Operator",
         "scope": [
            "source.js constant.other.object.key.js string.unquoted.label.js"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#e9a149"
         }
      },
      {
         "name": "MARKDOWN - Heading Name Section",
         "scope": [
            "entity.name.section.markdown",
            "markup.heading.setext.1.markdown",
            "markup.heading.setext.2.markdown"
         ],
         "settings": {
            "foreground": "#e9a149",
            "fontStyle": "bold"
         }
      },
      {
         "name": "Markdown - Blockquote",
         "scope": [
            "markup.quote punctuation.definition.blockquote.markdown"
         ],
         "settings": {
            "background": "#e9a149",
            "foreground": "#e9a149"
         }
      },
      {
         "name": "Markdown - Separator",
         "scope": [
            "meta.separator"
         ],
         "settings": {
            "fontStyle": "bold",
            "background": "#00000050",
            "foreground": "#e9a149"
         }
      },
      {
         "scope": [
            "constant",
            "variable.other.constant",
            "source.js support.constant",
            "entity.other.attribute-name.class",
            "punctuation.definition.entity",
            "support.variable.property.dom",
            "punctuation.definition.template-expression",
            "constant.character.entity",
            "support.function.dom"
         ],
         "settings": {
            "foreground": "#b38300"
         }
      },
      {
         "scope": [
            "text.html.basic entity.other.attribute-name.html",
            "text.html.basic entity.other.attribute-name",
            "entity.other.attribute-name"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#b38300"
         }
      },
      {
         "name": "Markup - Bold",
         "scope": [
            "markup.bold",
            "markup.bold string"
         ],
         "settings": {
            "fontStyle": "bold",
            "foreground": "#b38300"
         }
      },
      {
         "name": "Markup - Bold-Italic",
         "scope": [
            "markup.bold markup.italic",
            "markup.italic markup.bold",
            "markup.quote markup.bold",
            "markup.bold markup.italic string",
            "markup.italic markup.bold string",
            "markup.quote markup.bold string"
         ],
         "settings": {
            "fontStyle": "bold italic",
            "foreground": "#b38300"
         }
      },
      {
         "scope": [
            "variable.function",
            "keyword.other.special-method",
            "support.function",
            "support.function.console",
            "support.type.vendored",
            "source.css support.type.property-name",
            "source.sass support.type.property-name",
            "source.scss support.type.property-name",
            "source.less support.type.property-name",
            "source.stylus support.type.property-name",
            "source.postcss support.type.property-name",
            "string.other.link.title.markdown",
            "string.other.link.description.markdown",
            "string.other.link.description.title.markdown",
            "keyword.other.unit"
         ],
         "settings": {
            "foreground": "#00899e"
         }
      },
      {
         "scope": [
            "entity.name.function",
            "entity.name.type",
            "source.cs entity.name.type",
            "support",
            "support.other.variable",
            "string.other.link"
         ],
         "settings": {
            "foreground": "#00899e",
            "fontStyle": "none"
         }
      },
      {
         "name": "Invalid, Exception",
         "scope": [
            "invalid",
            "invalid.illegal",
            "support.type.exception"
         ],
         "settings": {
            "foreground": "#c9481d"
         }
      },
      {
         "scope": [
            "variable.language",
            "source.cs keyword.other.this",
            "markup.italic.markdown",
            "variable.parameter.function.language.special.self.python"
         ],
         "settings": {
            "foreground": "#ff5b1a"
         }
      },
      {
         "scope": [
            "storage.type",
            "storage.type.function",
            "entity.name.tag"
         ],
         "settings": {
            "fontStyle": "none",
            "foreground": "#ff5b1a"
         }
      },
      {
         "name": "Numbers, Booleans, CSS - Units",
         "scope": [
            "constant.numeric",
            "constant.language"
         ],
         "settings": {
            "foreground": "#5c49e9"
         }
      },
      {
         "name": "String, Symbols",
