  "tokenColors": [
    {
      "settings": {
        "foreground": "#eeffffff",
        "background": "#263238ff"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#84aeae"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "constant.character",
        "constant.escape",
